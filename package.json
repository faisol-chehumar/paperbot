{
  "name": "paperbot",
  "version": "1.0.0",
  "description": "A fully-featured discord bot template written in Typescript",
  "license": "MIT",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:clean && npm run build:compile && npm run build:plugins && npm run type:check",
    "build:start": "npm run build && npm run start",
    "build:compile": "swc src -d build -D && rimraf build/plugins/**/node_modules/",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "start:all": "run-p dev script:magic-prompt script:chatgpt-tokens",
    "dev": "cross-env NODE_ENV=development nodemon --ignore scripts --exec ts-node --swc src/main.ts || (sleep 1; touch src/main.ts)",
    "dev:test": "cross-env NODE_ENV=development nodemon --ignore 'src/commands/*.ts' --exec ts-node --swc src/main.ts",
    "dev:start": "NODE_ENV=production node -r @swc-node/register src/main.ts",
    "type:check": "tsc --pretty --skipLibCheck --noEmit",
    "lint": "eslint --ext .ts src",
    "lint:fix": "prettier --write '**/*.ts' && eslint . --ext .ts --fix",
    "fetch:voices": "curl 'https://api.uberduck.ai/voices?mode=tts-basic' > assets/voices.json",
    "script:chatgpt-tokens": "ts-node --swc scripts/chatgpt.ts",
    "script:chatgpt-tokens-reload": "nodemon --ignore auth.json --exec ts-node --swc scripts/chatgpt.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name",
    "magic-prompt": "cd scripts/ && poetry run magic-prompt",
    "generate:sd-types": "npx openapi-typescript ./openapi.json --output ./src/types/openapi.d.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@paperfeed/eslint-config": "^1.0.3",
    "@prisma/client": "^4.8.0",
    "@types/sharp": "^0.31.1",
    "chatgpt": "^3.3.3",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.37.24",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.10.2",
    "ffmpeg-static": "^5.1.0",
    "fuzzysort": "^2.0.4",
    "libsodium-wrappers": "^0.7.10",
    "lodash": "^4.17.21",
    "openai": "^3.1.0",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "puppeteer": "^19.4.1",
    "sharp": "^0.31.3",
    "sqlite3": "^5.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.24",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.30.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "swc-plugin-node-cjs-interop": "^0.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=8.0.0"
  }
}
